#include <stdio.h>
int main(){
	int i=1,num,contp=0,conti=0,resto;
	while(i<=132)
	{
		printf("digite um valor:\n");
		scanf("%d",&num);
		resto=num%2;
		if(resto==1)
			conti++;
		else
			contp++;
		i++;
	}
	printf("possui %d numero impar\n",conti);
	printf("possui %d numero par\n",contp);
	return 0;
}
