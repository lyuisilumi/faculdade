#include <stdio.h>
int main()
{
	int v=1;
	float per;
	int cont1=0,cont2=0,cont3=0,cont4=0,contn=0,contb=0,cont=0;
	while(v!=0)
	{
		printf("qual o candidato que voce deseja?\n");
			scanf("%d",&v);
		if(v==0)
			printf("finalizado o conjunto de votos\n");
		else
		{
			switch(v)
			{
				case 1:
					cont1++;
					break;
				case 2:
					cont2++;
					break;
				case 3:
					cont3++;
					break;
				case 4:
					cont4++;
					break;
				case 5:
					contn++;
					break;
				case 6:
					contb++;
					break;
			}
		}
		cont++;
	}
}
