#include <stdio.h>
int main()
{
float n1,n2,n3,m;
printf("qual as notas do aluno?\n");
scanf("%f%f%f",&n1,&n2,&n3);
m=(n1*2+n2*3+n3*5)/10;
if(m<5)
	printf ("sua media eh %.2f e seu conceito E",m);
if(m>=5 && m<6)
	printf("sua media eh %.2f e seu conceito D",m);
if(m>=6 && m<7)
	printf("sua media eh %.2f e seu conceito C",m);
if(m>=7 && m<8)
	printf("sua media eh %.2f e seu conceito B",m);
if(m>=8)
	printf("sua media eh %.2f e seu conceito A",m);
return 0;
}
