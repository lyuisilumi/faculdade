#include <stdio.h>
int main(){
	int a,b,c;
	printf("de 3 valores de um triangulo:\n");
	scanf("%d%d%d",&a,&b,&c);
	if(a>b+c||b>a+c||b>c+a)
		printf("nao compoem um triangulo\n");
	else
	{
		if(a==b==c)
			printf("triangulo equilatero\n");
		else
		{
			if(c!=a==b||a!=b==c||b!=c==a)
				printf("triangulo isosceles\n");
			else
				printf("triangulo escaleno\n");
		}
	}
return 0;	
}
